{
  "builds": [
    { "src": "frontend/**", "use": "@vercel/static", "config": { "distDir": "frontend" } }
  ],
  "routes": [
    { "src": "/css/(.*)", "dest": "/css/$1" },
    { "src": "/js/(.*)", "dest": "/js/$1" },
    { "src": "/imgs/(.*)", "dest": "/imgs/$1" },
    { "src": "/(.*)", "dest": "/html/login.html" }
  ]
}
