{
  "builds": [
    { "src": "/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/css/(.*)", "dest": "/css/$1" },
    { "src": "/js/(.*)", "dest": "/js/$1" },
    { "src": "/imgs/(.*)", "dest": "/imgs/$1" },
    { "src": "/html/(.*)", "dest": "/html/$1" },
    { "src": "/", "dest": "/html/login.html" }
  ]
}
