{
  "builds": [
    { "src": "frontend/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/frontend/css/(.*)", "dest": "/frontend/css/$1" },
    { "src": "/frontend/js/(.*)", "dest": "/frontend/js/$1" },
    { "src": "/frontend/imgs/(.*)", "dest": "/frontend/imgs/$1" },
    { "src": "/frontend/html/(.*)", "dest": "/frontend/html/$1" },
    { "src": "/", "dest": "/frontend/html/login.html" }
  ]
}
