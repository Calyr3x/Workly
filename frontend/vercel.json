{
  "builds": [
    { "src": "frontend/css/**", "use": "@vercel/static" },
    { "src": "frontend/js/**", "use": "@vercel/static" },
    { "src": "frontend/imgs/**", "use": "@vercel/static" },
    { "src": "frontend/html/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/css/(.*)", "dest": "/frontend/css/$1" },
    { "src": "/js/(.*)", "dest": "/frontend/js/$1" },
    { "src": "/imgs/(.*)", "dest": "/frontend/imgs/$1" },
    { "src": "/(.*)", "dest": "/frontend/html/login.html" }
  ]
}
